#!/bin/bash
while IFS='' read -r line; do
    words=$(echo $line | tr " " "\n")
    if [ $(echo "$words" | wc -l) -gt 1 ]; then
        first_word=$(echo "$words" | head -1)
        last_word=$(echo "$words" | tail -1)
        str="$first_word $last_word"
        check=$(echo $str | egrep "^[^ ]*([^ ]{2,})[^ ]*\1[^ ]*[ ][^ ]*\1\1[^ ]*$")
        if [ "$check" != "" ]; then
            echo "$line"
        fi 
    fi
done < $1
