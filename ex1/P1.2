echo -n "Enter first number: "
read x
echo -n "Enter second number: "
read y
echo -n "Enter third number: "
read z
#x=2
#y=3
#z=6
mul=$[x*y]
add=$[x+y]
sub=$[x-y]
div=$[x/y]
if [ $mul -eq $z ]; then
	echo "$x*$y=$z"
	exit
fi
if [ $add -eq $z ]; then
        echo "$x+$y=$z"
        exit
fi
if [ $div -eq $z ]; then
        echo "$x/$y=$z"
        exit
fi
if [ $sub -eq $z ]; then
        echo "$x-$y=$z"
        exit
fi
echo "Do not fit"

