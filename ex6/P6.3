#!/bin/bash
echo -n "Enter 4 parameter in format file-string1-string2-num: "
read cmd

file=$(echo $cmd|cut -d"-" -f1)
str1=$(echo $cmd|cut -d"-" -f2)
str2=$(echo $cmd|cut -d"-" -f3)
i=$(echo $cmd|cut -d"-" -f4)

>|temp3
for w in $(cat $file); do
    echo $w | egrep "^([^$str1]{$i}[$str2]{$i}[$str1]{1,$i})+$" >> temp3
done

sort temp3
