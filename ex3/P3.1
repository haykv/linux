#!/bin/bash
echo -n "Enter program name: "
read filename
echo -n "Enter list of numbers: "
read num_list
echo -n "" >| temp
for num in $num_list; do
   line_number=1
   while read line; do    
        if [ $num -eq $line_number ]; then            
            echo "$line" >> temp
        fi
        line_number=$[line_number+1]
    done < $filename
done
chmod +x temp
temp 2>|stderr.out
cat stderr.out

