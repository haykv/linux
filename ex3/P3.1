#!/bin/bash
if [ "$1" == "debug" ]; then
    num_list="4 2 6 1"
    num_list="4 6 1 2"
else
    echo -n "Enter list of numbers: "
    read num_list
fi
filename=Prog1
echo -n "" > temp
for num in $num_list; do
   line_number=1
   while read line; do    
        if [ $num -eq $line_number ]; then            
            echo $line >> temp
        fi
        line_number=$[line_number+1]
    done < $filename
done
chmod +x temp
temp

